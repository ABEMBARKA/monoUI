<UserControl x:Class="ReflectionTest.Page"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation" 
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml" 
    Width="800" Height="600">
    <Grid x:Name="LayoutRoot" Background="White">
        <StackPanel>
            <StackPanel Orientation="Horizontal">
                <Button Name="button1" Content="user-code type" Click="Button_Click"></Button>
                <Button Name="button2" Content="non-existant type" Click="Button_Click"></Button>
                <Button Name="button3" Content="corlib private platform type" Click="Button_Click"></Button>
                <Button Name="button4" Content="a11y private platform type" Click="Button_Click"></Button>
            </StackPanel>

            <Button Name="button5" Content="Load the MoonAtkBridge assembly" Click="Button_Click"></Button>
        </StackPanel>

    </Grid>
</UserControl>
