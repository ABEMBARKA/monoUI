#!/bin/sh
# Run this to set configure.in up for an API version.

#this is a slightly modified version of this file: http://anonsvn.mono-project.com/viewvc/branches/gtk-sharp-2-468-branch/gtk-sharp/bootstrap-2.8?revision=114967
GTK_SHARP_VERSION=2.8.6
ASSEMBLY_VERSION=2.8.0.0
POLICY_VERSIONS="2.4 2.6"
GTK_REQUIRED_VERSION=2.8.0
GNOME_REQUIRED_VERSION=2.10.0
VERSIONCSDEFINES="-define:GTK_SHARP_2_6 -define:GTK_SHARP_2_8"
VERSIONCFLAGS="-DGTK_SHARP_2_6 -DGTK_SHARP_2_8"
GTK_API_TAG=2.8
GNOME_API_TAG=2.10

GLIB_REQUIRED_VERSION=2.8.11

. ./bootstrap-generic "$@"

